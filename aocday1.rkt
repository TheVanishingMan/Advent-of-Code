      ((eqv? current-floor -1) (values position current-floor))
      ((eqv? (string-ref str 0) #\() (basement?-helper (substring str 1) (add1 current-floor) (add1 position)))
      ((eqv? (string-ref str 0) #\)) (basement?-helper (substring str 1) (sub1 current-floor) (add1 position)))
      (else "panic")
      )))

